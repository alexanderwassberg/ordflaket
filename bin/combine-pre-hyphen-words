#!/usr/bin/perl
# vim: ft=perl:fdm=marker:fmr=#<,#>:fen:et:sw=2:
use strict;
use warnings FATAL => 'all';
use vars     qw($VERSION);
use autodie  qw(:all);

use utf8;
use open qw(:std :utf8);

use 5.012;
use List::MoreUtils qw(natatime);

chomp(my @words = <DATA>);

# modify this code file with the new words by using select on this filehandle.
# the new words will be added to the __DATA__ block.
# open(my $fh, '>>', $0) or die $!;

my $iterator = natatime(2, @words);

my (@pre_hyphen, @post_hyphen);
while(my ($first, $second) = $iterator->()) {
	push(@pre_hyphen,  ($first =~ m/(.+)[-]/));
	push(@post_hyphen, ($first =~ m/[-](.+)$/));
}

{
  no warnings 'uninitialized';
#  select $fh;
  while(my ($i, $newpre) = (each(@pre_hyphen))) {
    printf "%s-%s\n", $newpre, defined($pre_hyphen[$i +1]) ? $pre_hyphen[$i + 1] : $pre_hyphen[$i -2];
  }
  while(my ($j, $newpost) = (each(@post_hyphen))) {
    printf "%s-%s\n", $newpost, defined($post_hyphen[$j +1]) ? $post_hyphen[$j + 1] : $post_hyphen[$j - 2];
  }
}


__END__
